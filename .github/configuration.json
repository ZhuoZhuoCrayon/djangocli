{
    "categories": [
      {
        "title": "### 🔥 Refactors",
        "labels": ["kind/refactor", "refactor"]
      },
      {
        "title": "### 🚀 Features",
        "labels": ["kind/feature", "feature"]
      },
      {
        "title": "### ✨ Optimizations",
        "labels": ["kind/optimization", "optimization"]
      },
      {
        "title": "### 🐛 Bugfixes",
        "labels": ["kind/bugfix", "bugfix"]
      },
      {
        "title": "### ✏️ Docs",
        "labels": ["kind/docs", "docs"]
      },
      {
        "title": "### 🧪 Tests",
        "labels": ["kind/test", "test"]
      }
    ],
    "sort": "ASC",
    "template": "${{CHANGELOG}}\n**Full Changelog**: __GITHUB_REPO_URL__/compare/${{FROM_TAG}}...__TO_TAG___",
    "pr_template": "- ${{TITLE}}\n",
    "empty_template": "- no changes",
    "label_extractor": [
      {
        "pattern": "(feature|docs|bugfix|optimization|refactor|test): (.*)",
        "target": "$1"
      }
    ],
    "duplicate_filter": {
      "pattern": "github.*",
      "on_property": "author",
      "method": "match"
    }
}
